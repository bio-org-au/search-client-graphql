<!DOCTYPE html>
<html>
<head>
  <title>SearchClientGraphql</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <script>
    window.relative_url_root = "<%= Rails.application.config.relative_url_root %>";
  </script>
  <style media="print">
     body {color:#000000;
           background-color:#FFFFFF;} 
     .no-print {display: none;}
  </style>
      <% if development? %>
<style>
a:not([title]),
select:not([title]),
textarea:not([title]),
input:not([title]) {
  background: red;
}

input[type="checkbox"]:not([title]),
label:not([title]) {
  background: purple;
  color: red;
  border: 2px solid black;
  width: 16px;
  height: 16px;
}

table.for-search-form {
  border-collapse: collapse;
}
table.for-search-form td.label {
  text-align: right;
  padding: 0;
  margin: 0;
}


hr.heavy {
  height: 2px;
  background: darkgray;
}
</style>

    <% end %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>

</head>
<body>

<%= yield %>

</body>
</html>
