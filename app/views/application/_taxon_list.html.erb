
<ul class="plain">
  <% @results.taxa.each do |taxon| %>
    <li>
    <% if taxon.cross_reference? %>
      <%= render(partial: 'application/taxon_list/cross_reference', locals: {taxon: taxon}) %>
    <% else %>
      <%= render(partial: 'application/taxon_list/direct_reference', locals: {taxon: taxon}) %>
    <% end %>
  <% end %>
</ul>
